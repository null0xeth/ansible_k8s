---
apiVersion: traefik.io/v1alpha1
kind: IngressRoute
metadata:
  name: harbor-ui
  namespace: harbor
  annotations:
    kubernetes.io/ingress.class: traefik-external
spec:
  entryPoints: [websecure]
  routes:
    - match: Host(`harbor.chonk.city`)
      kind: Rule
      services:
        - name: harbor
          port: 443
          strategy: RoundRobin
          weight: 10
  tls:
    secretName: chonk-city-tls



